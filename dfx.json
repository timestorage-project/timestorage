{
  "canisters": {
    "timestorage_backend": {
      "main": "src/timestorage_backend/main.mo",
      "type": "motoko",
      "motoko": {
        "version": "0.8.6"
      }
    },
    "timestorage_frontend": {
      "dependencies": ["timestorage_backend", "internet-identity"],
      "source": ["src/timestorage_frontend/dist"],
      "type": "assets",
      "workspace": "timestorage_frontend"
    },
    "timestorage_admin": {
      "dependencies": [
        "timestorage_backend",
        "internet-identity",
        "timestorage_frontend"
      ],
      "source": ["src/timestorage_admin/dist"],
      "type": "assets",
      "workspace": "timestorage_admin"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral",
      "canisters": {
        "test_runner": {
          "main": "test/test.mo",
          "type": "motoko",
          "dependencies": ["timestorage_backend"]
        },
        "internet-identity": {
          "type": "pull",
          "id": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      }
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}
